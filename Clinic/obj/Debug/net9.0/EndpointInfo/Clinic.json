    "/api/User/Register": {
      "post": {
        "tags": [
          "User"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "ConfirmPassword",
                  "Email",
                  "Name",
                  "Nickname",
                  "Password",
                  "Phone",
                  "Photo",
                  "UserName"
                ],
                "type": "object",
                "properties": {
                  "Name": {
                    "type": "string"
                  },
                  "Nickname": {
                    "type": "string"
                  },
                  "Email": {
                    "type": "string"
                  },
                  "UserName": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string"
                  },
                  "ConfirmPassword": {
                    "type": "string"
                  },
                  "DateOfBirth": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "Gender": {
                    "$ref": "#/components/schemas/Gender"
                  },
                  "Photo": {
                    "type": "string",
                    "format": "binary"
                  },
                  "Phone": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "Name": {
                  "style": "form"
                },
                "Nickname": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "UserName": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "ConfirmPassword": {
                  "style": "form"
                },
                "DateOfBirth": {
                  "style": "form"
                },
                "Gender": {
                  "style": "form"
                },
                "Photo": {
                  "style": "form"
                },
                "Phone": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
